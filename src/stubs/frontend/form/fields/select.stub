                            outlined
                            map-options
                            emit-value
                            clearable{{ IS_MULTI_SELECT }}{{ IS_USE_CHIPS }}
                        >
                            <template v-slot:label>
                                {{ $t('{{ LABEL }}') }} {{ REQUIRED_HTML }}
                            </template>
                        </q-select>
                    </v-field>
                </div>
