
                <!-- ================= {{ NAME }} ================= -->
                <div class="col-12 col-md-6 col-padding">
                    <v-field name="{{ NAME }}" :label="$t('{{ LABEL }}').toLowerCase()" v-slot="{ field, errorMessage }">
