                                    
                                    <!-- ================= {{ NAME }} ================= -->
                                    <div class="{{ CLASS_FIELD }}">
                                        <div class="item">
                                            <div class="label" v-text="$t('{{ LABEL }}')"></div>
                                            <div class="value">
                                                <q-badge class="px-2 py-1" :class="{'bg-green-7': model.{{ NAME }}, 'bg-grey': !model.{{ NAME }}}" rounded>
